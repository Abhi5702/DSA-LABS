package com.myjlc.loops;

public class Lab3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		String Str1= "abhi";
		String Str2= "nav";
		
		System.out.println(Str1+"\t"+Str2);

		String Temp = Str1;
		Str1=Str2;
		Str2= Temp;
		
		System.out.println(Str1+"\t"+Str2);
	}

}
